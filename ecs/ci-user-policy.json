      "Resource": [
        "arn:aws:iam::992167236365:role/ecsTaskExecutionRole-wafr",
        "arn:aws:iam::992167236365:role/ecsTaskRole-wafr"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:*"
      ],
      "Resource": "*"
    }
  ]
}
